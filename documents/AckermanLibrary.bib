@article{Lamb2024,
  title = {Comparing Standard- and Reduced-size Passive Integrated Transponder ({{PIT}}) Tags for Monitoring Juvenile Wild Spring {{Chinook Salmon}}},
  author = {Lamb, Jesse J. and Sandford, Benjamin P. and Smith, Steven G. and Axel, Gordon A.},
