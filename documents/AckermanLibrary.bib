@article{Waterhouse2020,
  title = {A {{Bayesian}} Nested Patch Occupancy Model to Estimate Steelhead Movement and Abundance},
  author = {Waterhouse, Lynn and White, Jody and See, Kevin and Murdoch, Andrew and Semmens, Brice X.},
  year = {2020},
