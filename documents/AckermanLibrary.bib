@article{Horn2024,
  title = {Utility of Parentage-based Tagging for Monitoring {{Coho}} Salmon ( {{{\emph{Oncorhynchus}}}}{\emph{ Kisutch}} ) in the Interior {{Columbia River}} Basin},
  author = {Horn, Rebekah L. and Nuetzel, Hayley M. and Johnson, Becky and Kamphaus, Cory and Lovrak, Jon and Mott, Kraig and Newsome, Todd and Narum, Shawn R.},
  year = {2024},
  month = feb,
  journal = {Evolutionary Applications},
  volume = {17},
