@article{Brieuc2015,
  title = {Integration of {{Random Forest}} with Population-Based Outlier Analyses Provides Insight on the Genomic Basis and Evolution of Run Timing in {{Chinook}} Salmon ( {{{\emph{Oncorhynchus}}}}{\emph{ Tshawytscha}} )},
  author = {Brieuc, Marine S. O. and Ono, Kotaro and Drinan, Daniel P. and Naish, Kerry A.},
  year = {2015},
